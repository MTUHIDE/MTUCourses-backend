{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "NODE_ENV=dev nest start --watch",
    "dev:debug": "NODE_ENV=dev nest start --debug --watch",
    "dev:no-processors": "DISABLE_PROCESSORS=true yarn dev",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "ava",
    "test:watch": "ava -w",
    "test:coverage": "c8 ava",
    "migrations:generate": "prisma migrate dev",
    "migrations:run": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "prod:prepare": "yarn migrations:run && yarn db:seed"
  },
  "dependencies": {
    "@fastify/static": "6.5.0",
    "@mtucourses/rate-my-professors": "^2.0.1",
    "@mtucourses/scraper": "^3.0.0",
    "@mtucourses/thumbor": "^2.0.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/platform-fastify": "^9.1.4",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@prisma/client": "^4.0.0",
    "@rschedule/core": "^1.2.3",
    "@rschedule/json-tools": "^1.2.3",
    "@rschedule/standard-date-adapter": "^1.2.3",
    "arr-diff": "^4.0.0",
    "cache-manager": "^3.4.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dd-trace": "^1.5.0",
    "deep-equal": "^2.0.5",
    "fastify-swagger": "^3.5.0",
    "got": "^11.8.2",
    "graphile-worker": "0.13.0",
    "nestjs-graphile-worker": "0.2.0",
    "p-throttle": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-node": "10.9.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.1.4",
    "@types/arr-diff": "^4.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^1.7.3",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "ava": "4.3.3",
    "ava-postgres": "1.0.4",
    "c8": "7.12.0",
    "delay": "^4.4.0",
    "eslint": "^7.32.0",
    "eslint-config-xo-typescript": "^0.38.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "glob": "8.0.3",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prisma": "^4.0.0",
    "supertest": "^6.0.0",
    "ts-loader": "^9.2.5",
    "tsconfig-paths": "4.1.0",
    "type-fest": "^1.0.1",
    "typescript": "^4.8.4",
    "webpack": "^5.50.0",
    "xo": "^0.52.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "xo --fix"
    ]
  },
  "xo": {
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "new-cap": [
        "error",
        {
          "capIsNewExceptionPattern": "@*"
        }
      ],
      "import/extensions": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports-ts": "error",
      "unicorn/no-abusive-eslint-disable": "off",
      "unused-imports/no-unused-vars-ts": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ],
      "@typescript-eslint/comma-dangle": "off",
      "@typescript-eslint/naming-convention": "off",
      "n/prefer-global/process": "off",
      "unicorn/no-await-expression-member": "off"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
