{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "prisma generate",
    "build": "nest build",
    "start": "node dist/src/main",
    "dev": "NODE_ENV=dev nest start --watch",
    "dev:debug": "NODE_ENV=dev nest start --debug --watch",
    "dev:no-processors": "DISABLE_PROCESSORS=true yarn dev",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "ava",
    "test:watch": "ava -w",
    "test:coverage": "c8 ava",
    "migrations:generate": "prisma migrate dev",
    "migrations:run": "prisma migrate deploy",
    "typings:generate": "zapatos",
    "db:seed": "prisma db seed",
    "prod:prepare": "yarn migrations:run && yarn db:seed"
  },
  "dependencies": {
    "@fastify/static": "7.0.1",
    "@mtucourses/rate-my-professors": "^2.0.1",
    "@mtucourses/scraper": "^3.0.3",
    "@mtucourses/thumbor": "^2.0.0",
    "@nestjs/cache-manager": "2.2.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.5",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.5",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/platform-fastify": "^10.3.5",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "^4.5.0",
    "@rschedule/core": "^1.2.3",
    "@rschedule/json-tools": "^1.2.3",
    "@rschedule/standard-date-adapter": "^1.2.3",
    "@sentry/node": "7.76.0",
    "@sentry/profiling-node": "7.108.0",
    "@sentry/tracing": "7.76.0",
    "JSONStream": "1.3.5",
    "arr-diff": "^4.0.0",
    "cache-manager": "5.4.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "deep-equal": "^2.0.5",
    "fastify": "4.26.2",
    "fastify-swagger": "^5.2.0",
    "got": "^11.8.2",
    "graphile-worker": "0.13.0",
    "nestjs-graphile-worker": "0.5.1",
    "p-retry": "4.6.2",
    "p-throttle": "^4.1.1",
    "p-timeout": "4.1.0",
    "pg": "8.8.0",
    "pg-query-stream": "4.2.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-node": "10.9.1",
    "zapatos": "6.1.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.5",
    "@swc-node/register": "1.5.4",
    "@swc/core": "1.3.9",
    "@types/arr-diff": "^4.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^1.7.3",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/glob": "8.1.0",
    "@types/node": "^16.6.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "ava": "4.3.3",
    "ava-postgres": "1.0.4",
    "c8": "7.12.0",
    "delay": "^4.4.0",
    "esbuild": "0.15.12",
    "esbuild-runner": "2.2.2",
    "eslint": "^8.57.0",
    "eslint-config-xo-typescript": "^4.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "glob": "8.0.3",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prisma": "^4.5.0",
    "supertest": "^6.0.0",
    "ts-loader": "^9.2.5",
    "tsconfig-paths": "4.1.0",
    "type-fest": "^1.0.1",
    "typescript": "^5.4.3",
    "webpack": "^5.50.0",
    "xo": "^0.58.0"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "xo --fix"
    ]
  },
  "xo": {
    "plugins": [
      "unused-imports"
    ],
    "ignore": [
      "src/zapatos/**"
    ],
    "rules": {
      "new-cap": [
        "error",
        {
          "capIsNewExceptionPattern": "@*"
        }
      ],
      "n/file-extension-in-import": "off",
      "import/extensions": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports-ts": "error",
      "unicorn/no-abusive-eslint-disable": "off",
      "unused-imports/no-unused-vars-ts": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": true
        }
      ],
      "@typescript-eslint/comma-dangle": "off",
      "@typescript-eslint/naming-convention": "off",
      "n/prefer-global/process": "off",
      "unicorn/no-await-expression-member": "off",
      "@typescript-eslint/no-unsafe-argument": "off"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
